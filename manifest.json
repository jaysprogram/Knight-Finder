{
    "manifest_version": 3,
    "name": "Knight Search",
    "version": "1.0",
    "description": "A simple chrome extension",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Images/icon16.png",
            "48": "Images/icon48.png",
            "128": "Images/icon128.png"
        }
    },
    
    "permissions": [
        "storage"
    ]

}


